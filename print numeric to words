#include<stdio.h>
#include<conio.h>
void main()
{
int a;
printf("Enter the value(1-5):");
scanf("%d",&a);
switch(a)
{
case 1:
printf("one");
break;
case 2:
printf("\n Two");
break;
case 3:
printf("\n Three");
break;
case 4:
printf("\n Four");
break;
case 5:
printf("\n Five");
break;
default:
printf("\n Error");
}
}
